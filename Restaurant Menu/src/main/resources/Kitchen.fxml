<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="1000.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.rhul.cs2810.gui.KitchenController">
   <children>
      <Label fx:id="exitLabel" alignment="CENTER" contentDisplay="CENTER" minWidth="-Infinity" prefHeight="40.0" prefWidth="1000.0" style="-fx-background-color: black;" text="Press [e] to return to login" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Consolas" size="14.0" />
         </font>
      </Label>
      <Button fx:id="orderHistoryButton" mnemonicParsing="false" onAction="#createHistory" style="-fx-background-color: gray;" text="Order History" />
      <Label fx:id="clockLabel" alignment="CENTER" contentDisplay="CENTER" minWidth="-Infinity" prefHeight="40.0" prefWidth="1000.0" style="-fx-background-color: black;" text="00:00" textFill="#00e200">
         <font>
            <Font name="Consolas" size="34.0" />
         </font>
      </Label>
      <ScrollPane fx:id="ordersGridScrollpane" fitToHeight="true" fitToWidth="true" prefHeight="950.0" prefWidth="1000.0" style="-fx-background-color: black;" />
   </children>
</VBox>
